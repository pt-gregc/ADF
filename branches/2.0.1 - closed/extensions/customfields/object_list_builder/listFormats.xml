<?xml version="1.0" encoding="UTF-8"?>
<listFormats class="array">
	<format>
		<formatName>h4Listing</formatName>
		<displayName>Listing with H4</displayName>
		<structure><![CDATA[<h4>{CourseID} - {CourseTitle}</h4> <u>Course Instructor</u>: <b>{CourseInstructor}</b>]]></structure>
		<isDefault>0</isDefault>
		<wrapperTag>section</wrapperTag>
	</format>
	<format>
		<formatName>linkListing</formatName>
		<displayName>Listing with link</displayName>
		<structure><![CDATA[<a href="javascript:alert('hello')">{CourseID} - {CourseTitle}</a>]]></structure>
		<isDefault>1</isDefault>
		<wrapperTag>span</wrapperTag>
	</format>
	<format>
		<formatName>divListingWithCredits</formatName>
		<displayName>Listing with credits in a Div</displayName>
		<structure><![CDATA[<div><b>{CourseID} - {CourseTitle}</b> <span style="font-size:small;">(<u>Credits</u>: <b>{CourseCredits}</b>)</span></div>]]></structure>
		<isDefault>0</isDefault>
		<wrapperTag>div</wrapperTag>
	</format>
	<format>
		<formatName>pListingWithCredits</formatName>
		<displayName>Listing with credits in a P</displayName>
		<structure><![CDATA[<p><b>{CourseID} - {CourseTitle}</b></p> <span style="font-size:small;">(<u>Credits</u>: <b>{CourseCredits}</b>)</span>]]></structure>
		<isDefault>0</isDefault>
		<wrapperTag>p</wrapperTag>
	</format>	
	<format>
		<formatName>h3ListingInstructorDurationAndCredits</formatName>
		<displayName>Listing Instructor, Duration, Credits with H3</displayName>
		<structure><![CDATA[<h3>{CourseID} - {CourseTitle}</h3> <u>Course Instructor</u>: <b>{CourseInstructor}</b> <u>Duration</u>: <b>{CourseDuration}</b> <u>Credits</u>: <b>{CourseCredits}</b>]]></structure>
		<isDefault>0</isDefault>
		<wrapperTag>section</wrapperTag>
	</format>
	<format>
		<formatName>listingWithStyles</formatName>
		<displayName>Listing with styles</displayName>
		<structure><![CDATA[<div class="titleClass">{CourseID} - {CourseTitle}</div><div><span class="underLineText">Course Instructor</span>: <span class="boldText">{CourseInstructor}</span> <span class="underLineText">Duration</span>: <span class="boldText">{CourseDuration}</span> <span class="underLineText">Credits</span>: <span class="boldText">{CourseCredits}</span></div>]]></structure>
		<isDefault>0</isDefault>
		<wrapperTag>div</wrapperTag>
	</format>	
	<format>
		<formatName>title</formatName>
		<displayName>Title</displayName>
		<structure><![CDATA[{CourseTitle}]]></structure>
		<isDefault>0</isDefault>
	</format>	
</listFormats>