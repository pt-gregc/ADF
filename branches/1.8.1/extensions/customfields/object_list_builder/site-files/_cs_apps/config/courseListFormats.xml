<?xml version="1.0" encoding="UTF-8"?>
<listFormats class="array">
	<format>
		<formatName>titleCredits</formatName>
		<displayName>Course Title and Credits</displayName>
		<structure><![CDATA[{courseTitle} - {CourseCredits} Credit(s)]]></structure>
		<isDefault>1</isDefault>
		<wrapperTag>span</wrapperTag>
	</format>
	<format>
		<formatName>titleCode</formatName>
		<displayName>Course Title and Course Code</displayName>
		<structure><![CDATA[<div>{courseTitle}</div><div>Code: {courseCode}</div>]]></structure>
		<isDefault>0</isDefault>
		<wrapperTag>span</wrapperTag>
	</format>
	<format>
		<formatName>codeTitleInstructor</formatName> 
		<displayName>Listing with H4</displayName> 
		<structure><![CDATA[<h4>{courseCode} - {CourseTitle}</h4> <u>Course Instructor</u>: <b>{CourseInstructor}</b>]]></structure> 
		<isDefault>0</isDefault> 
		<wrapperTag>section</wrapperTag> 
	</format>
</listFormats>